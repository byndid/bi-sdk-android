    <style name="BeyondIdentityCardView" parent="CardView">
        <item name="cardBackgroundColor">@color/bi_sheet_background</item>
        <item name="android:outlineAmbientShadowColor">@color/bi_sheet_shadow</item>
        <item name="android:outlineSpotShadowColor">@color/bi_sheet_shadow</item>
        <item name="cardCornerRadius">@dimen/spacing_m</item>
        <item name="elevation">@dimen/spacing_m</item>
    </style>
    <style name="BiTextBase">
        <item name="android:fontFamily">@font/overpass</item>
        <item name="android:textColor">@color/neutral_60</item>
        <item name="fontWeight">400</item>
    </style>
    <style name="BottomSheetStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@drawable/dialog_background</item>
    </style>
    <style name="HeadingM" parent="BiTextBase">
        <item name="android:textSize">@dimen/heading_m</item>
    </style>
    <style name="HeadingS" parent="BiTextBase">
        <item name="android:textSize">@dimen/heading_s</item>
    </style>
    <style name="HeadingS.Bold">
        <item name="fontWeight">700</item>
    </style>
    <style name="PrimaryButton" parent="TextM">
        <item name="android:background">@drawable/primary_button_background</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:foreground">?android:selectableItemBackground</item>
        <item name="android:textColor">@color/white_static</item>
    </style>
    <style name="PrimaryButtonDanger" parent="PrimaryButton">
        <item name="android:background">@drawable/primary_danger_button_background</item>
    </style>
    <style name="PrimaryInput" parent="TextM">
        <item name="android:background">@drawable/input_text_background</item>
        <item name="hintTextColor">@color/neutral_70</item>
    </style>
    <style name="TextL" parent="BiTextBase">
        <item name="android:textSize">@dimen/text_l</item>
    </style>
    <style name="TextL.Bold" parent="TextM">
        <item name="fontWeight">700</item>
    </style>
    <style name="TextM" parent="BiTextBase">
        <item name="android:textSize">@dimen/text_m</item>
    </style>
    <style name="TextM.Bold" parent="TextM">
        <item name="fontWeight">700</item>
    </style>
    <style name="TextS" parent="BiTextBase">
        <item name="android:textSize">@dimen/text_s</item>
    </style>
    <declare-styleable name="PinEntryEditText">
        <attr format="enum" name="pinAnimationType">
            <enum name="popIn" value="0"/>
            <enum name="fromBottom" value="1"/>
            <enum name="none" value="-1"/>
        </attr>
        <attr format="string" name="pinCharacterMask"/>
        <attr format="string" name="pinRepeatedHint"/>
        <attr format="dimension" name="pinLineStroke"/>
        <attr format="dimension" name="pinLineStrokeSelected"/>
        <attr format="dimension" name="pinCharacterSpacing"/>
        <attr format="dimension" name="pinTextBottomPadding"/>
        <attr format="color" name="pinLineColors"/>
        <attr format="reference" name="pinBackgroundDrawable"/>
        <attr format="boolean" name="pinBackgroundIsSquare"/>
    </declare-styleable>
